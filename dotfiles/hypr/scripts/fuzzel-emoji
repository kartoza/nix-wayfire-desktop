#!/usr/bin/env bash
# Slightly tweaked version of https://github.com/evanriley/fuzzel-emoji
set -euo pipefail

EMOJI="$(sed '1,/^### DATA ###$/d' $0 | fuzzel --dmenu --prompt="ó°… " --match-mode=exact --width=48 | cut -d ' ' -f 1 | tr -d '\n')"
wtype "$EMOJI"; wl-copy "$EMOJI"
exit
